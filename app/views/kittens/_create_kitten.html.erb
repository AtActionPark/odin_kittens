<%= render 'shared/error', object: @kitten %>
<%= form_for @kitten do |f| %>
  <%= f.label 'Name' %>
  <%= f.text_field :name %><br>

  <%= f.label 'Age' %>
  <%= f.text_field :age %><br>

  <%= f.label 'Cuteness' %>
  <%= f.text_field :cuteness %><br>

  <%= f.label 'Softness' %>
  <%= f.text_field :softness %><br>

  <%= f.submit("Submit") %>
<% end %>